<!-- Pagina del perfil de usuario administrador -->

<html>
<%- include('partials/dashmenuUV.ejs'); %>

  <body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <%- include('partials/dashhead.ejs'); %>

        <div class="box">
          <div class="box-header with-border">
            <h3>Perfil del vendedor</h3>
            <h4 class="text-muted">Aqu√≠ puedes ver tu perfil y algunos datos del mismo</h4>
            <a type="button" href="/vendedor/cambiar-datos"><i class="fa fa-pencil-square-o btn btn-primary btn-lg"> Modificar
                Perfil</i></a>
          </div>
          <div class="row">
          </div>
          <div class="row">
            <div class="col-md-4">
            </div>
            <div class="col-md-4">
              <div class="panel panel-default">
                <!-- Datos del administrador -->
                <div class="panel-body">
                  <% vendedor.forEach((element)=>{ %>
                  <h3>Nombre: <%= element.nombres%> <%= element.apellidos%></h3>
                  <p>Usuario: <%= element.usuario%></p>
                  <% }); %>
                  <div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- jQuery 3 -->
        <script src="v1/../bower_components/jquery/dist/jquery.min.js"></script>
        <!-- Bootstrap 3.3.7 -->
        <script src="v1/../bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- SlimScroll -->
        <script src="v1/../bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
        <!-- FastClick -->
        <script src="v1/../bower_components/fastclick/lib/fastclick.js"></script>
        <!-- AdminLTE App -->
        <script src="v1/../dist/js/adminlte.min.js"></script>
        <!-- AdminLTE for demo purposes -->
        <script src="v1/../dist/js/demo.js"></script>
    </div>
    <%- include('partials/dashfooter.ejs'); %>
      </script>
  </body>

</html>